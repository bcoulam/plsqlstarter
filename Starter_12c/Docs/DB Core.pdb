<o:DatabasePackageProcedure Id="o1042">
<a:ObjectID>34A57569-7B11-4B4C-90A2-CAC92B434291</a:ObjectID>
<a:Name>CHECK_DEBUG_TOGGLE</a:Name>
<a:Code>CHECK_DEBUG_TOGGLE</a:Code>
<a:CreationDate>1273769564</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:DatabasePackageProcedure.Public>0</a:DatabasePackageProcedure.Public>
<a:Definition>BEGIN
   gr_debug.last_checked_dtm := dt.get_sysdtm; --update timestamp no matter what

   -- Only read the table if debugging isn&#39;t being overidden by a manual
   -- call through set_dbg.
   IF (NOT gr_debug.session_override) THEN
      -- This query fails unless there is a &quot;Debug&quot; master parameter in APP_PARM,
      -- and an application-specific value for that paramter in APP_ENV_PARM.
      vld_debug_toggle_format(parm.get_val(DEBUG_PARM_NM));

   END IF;

END check_debug_toggle</a:Definition>
