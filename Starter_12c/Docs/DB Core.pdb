<o:DatabasePackageProcedure Id="o702">
<a:ObjectID>3AF86F0E-968B-4C93-A018-2A3BD1C1DBD0</a:ObjectID>
<a:Name>GET_TIME_DIFF</a:Name>
<a:Code>GET_TIME_DIFF</a:Code>
<a:CreationDate>1273769560</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>NUMBER</a:ReturnType>
<a:Definition>l_curr_dtm DATE;
   l_diff      NUMBER;
BEGIN
   IF (i_tm_uom NOT IN (&#39;second&#39;,&#39;minute&#39;,&#39;hour&#39;,&#39;day&#39;,&#39;week&#39;,&#39;year&#39;)) THEN
      raise_application_error(-20000,&#39;ERROR: Invalid i_tm_uom [&#39;||i_tm_uom||
         &#39;]. UOM parameter supports second, minute, hour, day, week and year.&#39;);
   END IF;

   IF (i_old_dtm IS NULL) THEN
      -- can&#39;t determine age if we don&#39;t have a start point, return no answer
      raise_application_error(-20000,&#39;ERROR: i_old_dtm is NULL. Cannot determine difference without a start point.&#39;);
   END IF;

   l_curr_dtm := NVL(i_curr_dtm, SYSDATE);
   SELECT (l_curr_dtm - i_old_dtm) * DECODE(i_tm_uom,
                                            &#39;day&#39;,1,
                                            &#39;hour&#39;,24,
                                            &#39;minute&#39;,1440,
                                            &#39;second&#39;,86400,
                                            &#39;week&#39;,(1/7),
                                            &#39;year&#39;,(1/365)
                                            ) age
     INTO l_diff
     FROM dual;

   RETURN l_diff;

END get_time_diff</a:Definition>
<a:BaseProcedure.Function>1</a:BaseProcedure.Function>
<c:Parameters>
<o:Parameter Id="o703">
<a:ObjectID>A57D589C-B1A5-48AC-AD5F-F635D11F6B6A</a:ObjectID>
<a:Name>I_OLD_DTM</a:Name>
<a:Code>I_OLD_DTM</a:Code>
