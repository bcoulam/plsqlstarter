<o:DatabasePackageProcedure Id="o390">
<a:ObjectID>5D28231A-E427-498C-B4BA-76B762BDF4AD</a:ObjectID>
<a:Name>GET_DB_VERSION</a:Name>
<a:Code>GET_DB_VERSION</a:Code>
<a:CreationDate>1273769553</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>NUMBER</a:ReturnType>
<a:Definition>l_version VARCHAR2(30);
   l_version_num INTEGER := 0;
BEGIN
   SELECT SUBSTR(banner,
                 INSTR(banner, CHR(9), 1, 1) + 1,
                 INSTR(banner, CHR(9), 1, 2) - INSTR(banner, CHR(9), 1, 1) - 1) version
     INTO l_version
     FROM v$version
    WHERE UPPER(banner) LIKE &#39;CORE%&#39;;
   IF (l_version LIKE &#39;9%&#39;) THEN
      l_version_num := 9;
   ELSIF (l_version LIKE &#39;10%&#39;) THEN
      l_version_num := 10;
   ELSIF (l_version LIKE &#39;11%&#39;) THEN
      l_version_num := 11;
   ELSIF (l_version LIKE &#39;8%&#39;) THEN
      l_version_num := 8;
   ELSIF (l_version LIKE &#39;7%&#39;) THEN
      l_version_num := 7;
   END IF;

   RETURN l_version_num;
END get_db_version</a:Definition>
<a:BaseProcedure.Function>1</a:BaseProcedure.Function>
</o:DatabasePackageProcedure>
<o:DatabasePackageProcedure Id="o391">
<a:ObjectID>09C6B113-C4A8-4BCC-8AE5-5DE7C81CC75A</a:ObjectID>
<a:Name>DATA_IS_FOUND</a:Name>
<a:Code>DATA_IS_FOUND</a:Code>
<a:CreationDate>1273769553</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>BOOLEAN</a:ReturnType>
<a:Definition>l_proc_nm user_objects.object_name%TYPE := &#39;data_is_found&#39;;
   l_obj_nm  user_objects.object_name%TYPE;
   l_count   INTEGER := 0;

   lx_partition_not_there EXCEPTION;
   PRAGMA EXCEPTION_INIT(lx_partition_not_there, -02149);
BEGIN
   l_obj_nm := UPPER(i_tbl_nm);

   IF (NOT obj_exists(l_obj_nm, gc_table)) THEN
      -- If table doesn&#39;t exist, we&#39;ll pretend it does and has no data.
      -- Otherwise, calls to drop_tbl end up spitting out two similar messages
      l_count := 0;
   ELSE
      EXECUTE IMMEDIATE &#39;SELECT COUNT(*) &#39; || &#39;FROM &#39; || l_obj_nm ||
                        ifnn(i_part_nm, &#39; PARTITION (&#39; || i_part_nm || &#39;) &#39;) ||
                        &#39; WHERE ROWNUM &lt;= 1&#39;
         INTO l_count;
   END IF;

   IF (l_count &gt; 0) THEN
      RETURN TRUE;
   ELSE
      RETURN FALSE;
   END IF;
EXCEPTION
   WHEN lx_partition_not_there then
      err(l_proc_nm || g_sep_char || &#39;Partition &#39;||l_obj_nm||&#39;.&#39;||UPPER(i_part_nm)||&#39; not found.&#39;);
END data_is_found</a:Definition>
<a:BaseProcedure.Function>1</a:BaseProcedure.Function>
