<a:ObjectID>87EA78A3-85D0-46C4-8177-B0A72ADF22B0</a:ObjectID>
<a:Name>READ_LOCK</a:Name>
<a:Code>READ_LOCK</a:Code>
<a:CreationDate>1273769563</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>app_lock%ROWTYPE</a:ReturnType>
<a:Definition>lr_lock app_lock%ROWTYPE;
BEGIN
   SELECT *
     INTO lr_lock
     FROM app_lock
    WHERE lock_nm = i_lock_nm
      AND locked_obj_id IS NULL
      AND locked_obj_rid IS NULL;

   RETURN lr_lock;
EXCEPTION
   WHEN NO_DATA_FOUND THEN
      RETURN NULL; -- will be empty if lock wasn&#39;t found
   -- Let error bubble up if more than one match was found. This is possible
   -- with large-grained locks. But the error should point out where the large-
   -- grained lock is being used incorrectly.
END read_lock</a:Definition>
