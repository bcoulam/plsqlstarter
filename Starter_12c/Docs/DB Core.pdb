<a:Text>create trigger app_bi
  BEFORE INSERT ON app
  FOR EACH ROW
DECLARE
BEGIN
   IF (:new.app_id IS NULL) THEN
      SELECT app_seq.NEXTVAL INTO :new.app_id FROM dual;
   END IF;
END app_bi;
