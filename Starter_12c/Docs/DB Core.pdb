<o:DatabasePackageProcedure Id="o1202">
<a:ObjectID>AFCDA2F9-277C-4A98-98B4-4BDC389AEB45</a:ObjectID>
<a:Name>EWC</a:Name>
<a:Code>EWC</a:Code>
<a:CreationDate>1273769567</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>VARCHAR2</a:ReturnType>
<a:Definition>s VARCHAR2 (2000) := NVL(i_str,&#39;&#39;);
   slen PLS_INTEGER := NVL(length(i_str),0);
   diff  PLS_INTEGER := 0;
BEGIN
   IF (slen &lt; i_colsize) THEN
      diff := i_colsize - slen;

      FOR i IN 1..diff LOOP
         s := s || &#39; &#39;;
      END LOOP;
   ELSIF (slen &gt; i_colsize) THEN
      s := substr(s,1,i_colsize);
   END IF;

   RETURN s;
END ewc</a:Definition>
