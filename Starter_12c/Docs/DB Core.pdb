<o:Parameter Id="o483">
<a:ObjectID>614B6A25-2473-49F9-9251-9626EBC62E6D</a:ObjectID>
<a:Name>I_NEW_TABLESPACE</a:Name>
<a:Code>I_NEW_TABLESPACE</a:Code>
