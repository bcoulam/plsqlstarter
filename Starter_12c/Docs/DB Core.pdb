<o:DatabasePackageProcedure Id="o525">
<a:ObjectID>3FCCAB59-4265-43E4-BEDC-581470F19340</a:ObjectID>
<a:Name>ANALYZE_SCHEMA</a:Name>
<a:Code>ANALYZE_SCHEMA</a:Code>
<a:CreationDate>1273769553</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:Definition>l_proc_nm user_objects.object_name%TYPE := &#39;analyze_schema&#39;;
   l_objlist DBMS_STATS.objecttab;
BEGIN
   tag_session(&#39;DDL_UTILS&#39;,&#39;analyze_schema&#39;,&#39;Stale:&#39;||bool_to_str(i_stale));

   -- Just in case the client locked it
   IF (get_db_version &gt;= 10) THEN
      EXECUTE IMMEDIATE &#39;BEGIN DBMS_STATS.UNLOCK_SCHEMA_STATS(ownname =&gt; USER); END;&#39;;
   END IF;

   IF (i_new) THEN
      -- Analyzes the whole schema. Really only meant for new installs where no
      -- prior statistics exist.
      p(&#39;Analyzing new objects in schema &#39;||USER||&#39;...&#39;);
      DBMS_STATS.gather_schema_stats(
         ownname          =&gt; USER,
         estimate_percent =&gt; 1, --DBMS_STATS.auto_sample_size,
         DEGREE           =&gt; DBMS_STATS.default_degree,--DBMS_STATS.auto_degree,
         method_opt       =&gt; &#39;FOR ALL COLUMNS SIZE AUTO&#39;, -- the default
         granularity      =&gt; &#39;ALL&#39;,
         CASCADE          =&gt; TRUE,
         options          =&gt; &#39;GATHER EMPTY&#39;,
         objlist          =&gt; l_objlist
      );

      IF (l_objlist IS NOT NULL AND l_objlist.COUNT &gt; 0) THEN
         FOR i IN l_objlist.FIRST .. l_objlist.LAST LOOP
            p(&#39;Analyzed &#39; || LOWER(l_objlist(i).objtype) ||&#39; &#39;||l_objlist(i).objname||
               ifnn(l_objlist(i).partname,
                    &#39; (&#39;||l_objlist(i).partname||
                       ifnn(l_objlist(i).subpartname,&#39;.&#39;||l_objlist(i).subpartname, NULL) ||
                    &#39;)&#39;,
                    NULL
               )
             );
         END LOOP;
      ELSE
         inf(l_proc_nm || g_sep_char || &#39;No objects are missing statistics.&#39;);
      END IF;

   END IF;

   -- empty out results container
   l_objlist.DELETE;

   IF (i_stale) THEN
      -- User has requested stale stats only for the schema. Assumes monitoring
      -- has been turned on for the tables.
      p(&#39;Analyzing stale objects in schema &#39;||USER||&#39;...&#39;);

      -- If the system has been up a while, flush in-memory monitoring info
      -- to the dictionary before asking it determine what is and is not stale.
      -- Errata: Currently getting privileges error message. So we&#39;ll just have
      -- to ignore this.
      -- DBMS_STATS.flush_database_monitoring_info;

      DBMS_STATS.gather_schema_stats(
         ownname          =&gt; USER,
         estimate_percent =&gt; 1, --DBMS_STATS.auto_sample_size,
         DEGREE           =&gt; DBMS_STATS.default_degree, --DBMS_STATS.auto_degree,
         method_opt       =&gt; &#39;FOR ALL COLUMNS SIZE AUTO&#39;, -- the default
         granularity      =&gt; &#39;ALL&#39;,
         CASCADE          =&gt; TRUE,
         options          =&gt; &#39;GATHER STALE&#39;,
         objlist          =&gt; l_objlist
      );

      IF (l_objlist IS NOT NULL AND l_objlist.COUNT &gt; 0) THEN
         FOR i IN l_objlist.FIRST .. l_objlist.LAST LOOP
            p(&#39;Analyzed &#39; || LOWER(l_objlist(i).objtype) ||&#39; &#39;||l_objlist(i).objname||
               ifnn(l_objlist(i).partname,
                    &#39; (&#39;||l_objlist(i).partname||
                       ifnn(l_objlist(i).subpartname,&#39;.&#39;||l_objlist(i).subpartname, NULL) ||
                    &#39;)&#39;,
                    NULL
               )
             );
         END LOOP;
      ELSE
         inf(l_proc_nm || g_sep_char || &#39;No stale objects warranting fresh statistics&#39;);
      END IF;

   END IF;

   p(USER||&#39; analyzed.&#39;);

   untag_session;

END analyze_schema</a:Definition>
