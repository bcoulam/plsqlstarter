<a:CreationDate>1273769567</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>VARCHAR2</a:ReturnType>
<a:Definition>l_str_len INTEGER := LENGTH(i_str);
   l_width INTEGER := NVL(i_page_width,cnst.PAGEWIDTH);
   l_char VARCHAR2(1) := NVL(i_char,SP);
   l_space_left INTEGER := 0;
BEGIN
   l_space_left := l_width - l_str_len;

   IF (l_space_left &lt;= 0) THEN
      -- Can&#39;t do anything with is, already longer than page width
      RETURN i_str;
   ELSE
      RETURN LPAD(RPAD(i_str,l_str_len+(l_space_left/2),l_char),l_width,l_char);
   END IF;
END ctr</a:Definition>
