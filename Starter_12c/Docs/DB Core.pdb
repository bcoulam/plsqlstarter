BEGIN

   SELECT msg
     INTO l_message
     FROM app_msg
    WHERE msg_id = i_msg_id;

   RETURN l_message;

EXCEPTION
   WHEN NO_DATA_FOUND THEN
      -- Could have recursively called get_msg(MISSING_MSG_CD) here,
