<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>BOOLEAN</a:ReturnType>
<a:Definition>l_proc_nm  app_log.routine_nm%TYPE := gc_pkg_nm||&#39;.num_to_bool&#39;;
BEGIN
   IF (i_num = 0) THEN
      RETURN FALSE;
   ELSIF (i_num &lt;&gt; 0) THEN
      RETURN TRUE;
   ELSIF (i_num IS NULL) THEN
      RETURN NULL;
   ELSE
      logs.err(&#39;num_to_bool does not support [&#39;||i_num||&#39;]&#39;, TRUE, l_proc_nm, $$PLSQL_LINE);
   END IF;
END num_to_bool</a:Definition>
