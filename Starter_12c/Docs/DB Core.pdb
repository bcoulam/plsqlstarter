<o:DatabasePackageProcedure Id="o1163">
<a:ObjectID>E5ADC0AA-4670-4346-8818-6CD1A70C43AD</a:ObjectID>
<a:Name>MAKE_LIST</a:Name>
<a:Code>MAKE_LIST</a:Code>
<a:CreationDate>1273769566</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>VARCHAR2</a:ReturnType>
<a:Definition>l_str typ.t_maxvc2;
BEGIN
   IF (i_coll.COUNT &gt; 0) THEN
      FOR i IN i_coll.first..i_coll.last LOOP
         BEGIN
            IF (i &gt; 1) THEN
              l_str := l_str || i_delimiter || TO_CHAR(i_coll(i));
            ELSE -- only hit on first item
               l_str := TO_CHAR(i_coll(i));
            END IF;
         EXCEPTION
            WHEN NO_DATA_FOUND THEN --for deleted entries in nested table
               NULL; -- move on to next entry (if any)
         END;
      END LOOP;
   END IF;
   RETURN l_str;
END make_list</a:Definition>
