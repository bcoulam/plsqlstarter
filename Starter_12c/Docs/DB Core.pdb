   PRAGMA EXCEPTION_INIT(lx_catalog_invisible,-942);
BEGIN
   IF (get_db_version &lt; 10) THEN
      SELECT host_name
        INTO l_host_name
        FROM v$instance
       WHERE instance_number = sys_context(&#39;userenv&#39;, &#39;instance&#39;);
   ELSE
