<o:DatabasePackageProcedure Id="o655">
<a:ObjectID>DBA99693-A297-4FB4-9852-03C5F0CEB584</a:ObjectID>
<a:Name>GET_OBJ_USED_SPACE</a:Name>
<a:Code>GET_OBJ_USED_SPACE</a:Code>
<a:CreationDate>1273769555</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>NUMBER</a:ReturnType>
<a:DatabasePackageProcedure.Public>0</a:DatabasePackageProcedure.Public>
<a:Definition>l_proc_nm user_objects.object_name%TYPE := &#39;get_obj_used_space&#39;;
   l_used_space NUMBER;
BEGIN
   assert(obj_exists(i_obj_nm, i_obj_type),
      l_proc_nm || g_sep_char || INITCAP(i_obj_type)||&#39; &#39;||i_obj_nm||&#39; does not exist.&#39;);

   SELECT SUM(bytes)
     INTO l_used_space
     FROM user_segments
    WHERE segment_name = i_obj_nm;

   -- Add 25% as fudge factor. Certain operations could actually take double
   -- the amount, but I believe that happens in TEMP.
   l_used_space := l_used_space + TRUNC(l_used_space * .25);

   RETURN l_used_space;

END get_obj_used_space</a:Definition>
