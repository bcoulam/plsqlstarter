<o:DatabasePackageProcedure Id="o1345">
<a:ObjectID>5AC85405-943A-4BD4-8480-8762E606768D</a:ObjectID>
<a:Name>GET_MAX_PK_VAL</a:Name>
<a:Code>GET_MAX_PK_VAL</a:Code>
<a:CreationDate>1273769568</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>INTEGER</a:ReturnType>
<a:Definition>l_proc_nm  app_log.routine_nm%TYPE := gc_pkg_nm||&#39;.get_max_pk_val&#39;;
   l_pk_col_nm  user_cons_columns.column_name%TYPE;
   l_table_nm   user_tables.table_name%TYPE;
   l_max_pk_val INTEGER := 0;
BEGIN

   l_table_nm := UPPER(i_table_nm);

   excp.assert(
      i_expr =&gt; obj_exists(l_table_nm, gc_table),
      i_msg =&gt; &#39; Cannot find table &#39; || l_table_nm,
      i_routine_nm =&gt; l_proc_nm , i_line_num =&gt; $$PLSQL_LINE
      );

   BEGIN
      -- This SQL assumes the table has a single-column surrogate PK. The
      -- exception section handles those tables that don&#39;t conform to this scheme.
      SELECT column_name
        INTO l_pk_col_nm
        FROM user_cons_columns
       WHERE table_name = l_table_nm
         AND constraint_name =
             (SELECT constraint_name
                FROM user_constraints
               WHERE constraint_type = &#39;P&#39;
                 AND table_name = l_table_nm);
   EXCEPTION
      WHEN NO_DATA_FOUND THEN
         logs.err(&#39;There is no primary key for table &#39; ||l_table_nm
                 ,TRUE, l_proc_nm, $$PLSQL_LINE);
      WHEN TOO_MANY_ROWS THEN
         logs.err(&#39;Table &#39; || l_table_nm ||&#39; has a primary key that is composed of more than one column.&#39;
                 ,TRUE, l_proc_nm, $$PLSQL_LINE);
   END;

   EXECUTE IMMEDIATE &#39;SELECT MAX(&#39;||l_pk_col_nm||&#39;) FROM &#39; || l_table_nm
      INTO l_max_pk_val;

   RETURN l_max_pk_val;

END get_max_pk_val</a:Definition>
