<a:Definition>l_proc_nm user_objects.object_name%TYPE := &#39;get_cons_by_idx&#39;;
   l_idx_nm user_constraints.index_name%TYPE;
   l_cons_nm user_constraints.constraint_name%TYPE;
BEGIN
