<o:DatabasePackageProcedure Id="o1336">
<a:ObjectID>12D018C9-1C68-4A62-9E4E-E423F5D74E68</a:ObjectID>
<a:Name>CONVERT_CLOB_TO_BLOB</a:Name>
<a:Code>CONVERT_CLOB_TO_BLOB</a:Code>
<a:CreationDate>1273769568</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>BLOB</a:ReturnType>
<a:Definition>l_proc_nm  app_log.routine_nm%TYPE := gc_pkg_nm||&#39;.convert_clob_to_blob&#39;;

   ln_cloblen  INTEGER;
   l_blob   BLOB;

   -- vars for loop and conversion
   ln_offset NUMBER DEFAULT 1;
   ln_amount NUMBER DEFAULT 2000;
   l_rawchunk RAW(4000);
   ls_buffer VARCHAR2(32767);
BEGIN
   logs.dbg(&#39;Creating temporary BLOB...&#39;, l_proc_nm, $$PLSQL_LINE);
   dbms_lob.createtemporary(l_blob, TRUE);

   -- get clob length
   ln_cloblen := dbms_lob.getlength(i_clob);
   logs.dbg(&#39;CLOB is &#39;||ln_cloblen||&#39; bytes long.&#39;, l_proc_nm, $$PLSQL_LINE);

   LOOP
      EXIT WHEN ln_offset &gt; ln_cloblen;

      -- get buffer
      logs.dbg(&#39;Getting the next &#39;||ln_amount||&#39; bytes from CLOB starting at offset &#39;||ln_offset
              ,l_proc_nm, $$PLSQL_LINE);
      ls_buffer := dbms_lob.SUBSTR(i_clob, ln_amount, ln_offset);

      -- convert to raw
      logs.dbg(&#39;Casting buffer to RAW...&#39;, l_proc_nm, $$PLSQL_LINE);
      l_rawchunk := utl_raw.cast_to_raw(ls_buffer);

      -- write the converted data to the blob column
      logs.dbg(&#39;Writing RAW chunk to BLOB locator...&#39;, l_proc_nm, $$PLSQL_LINE);
      dbms_lob.WRITE(l_blob, utl_raw.length(l_rawchunk), ln_offset, l_rawchunk);

      ln_offset := ln_offset + ln_amount;
   END LOOP;

   RETURN l_blob;

END convert_clob_to_blob</a:Definition>
