<a:ObjectID>00731AF3-1208-4F76-98D2-95AF3FA63E9B</a:ObjectID>
<a:Name>I_TM_UOM</a:Name>
<a:Code>I_TM_UOM</a:Code>
<a:CreationDate>1273769560</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:DefaultValue>hour</a:DefaultValue>
<a:DataType>VARCHAR2</a:DataType>
