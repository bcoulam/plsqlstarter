<o:DatabasePackageProcedure Id="o793">
<a:ObjectID>641DF583-F4F4-4D32-8F6D-AF48C6280F64</a:ObjectID>
<a:Name>INIT_CLIENT_CTX</a:Name>
<a:Code>INIT_CLIENT_CTX</a:Code>
<a:CreationDate>1273769560</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:Definition>BEGIN
   -- Replicating functionality from the EXCP package, because ENV is one of the
   -- lowest layers in the framework, as must be independent.
   IF (LENGTH(i_client_id) &gt; 64 OR i_client_id IS NULL) THEN -- length as defined by data dictionary
      RAISE_APPLICATION_ERROR (-20000,&#39;ERROR: (Assertion Failure) [env.init_client_ctx]&#39;||
         &#39; User/Client ID should be &lt;= 64 characters&#39;);
   END IF;

   -- Populate in-memory structure with new identifier. If not passed in
   gr_client_ctx.client_id := i_client_id;

   IF (i_client_ip IS NOT NULL) THEN
      gr_client_ctx.client_ip := i_client_ip;
   END IF;
   IF (i_client_host IS NOT NULL) THEN
      gr_client_ctx.client_host := i_client_host;
   END IF;
   IF (i_client_os_user IS NOT NULL) THEN
      gr_client_ctx.client_os_user := i_client_os_user;
   END IF;
   IF (i_app_cd IS NOT NULL) THEN
      set_app_cd(i_app_cd);
   END IF;
   -- Populate USERENV with the given client identifier
   dbms_session.set_identifier(i_client_id);
END init_client_ctx</a:Definition>
