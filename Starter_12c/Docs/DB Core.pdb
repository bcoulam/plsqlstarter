      FOR lr IN (
         SELECT synonym_name
           FROM user_synonyms
          WHERE table_owner = UPPER(i_ref_owner)
      ) LOOP
         EXECUTE IMMEDIATE &#39;DROP SYNONYM &#39;||lr.synonym_name;
      END LOOP;
   END IF;
