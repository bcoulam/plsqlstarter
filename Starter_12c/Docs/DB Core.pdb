<a:Definition>ln_blocksize BINARY_INTEGER; -- will be discarded
   lb_exists BOOLEAN;
BEGIN
   utl_file.fgetattr(i_file_dir, i_file_nm, lb_exists, on_length, ln_blocksize);
   -- This check is necessary because fgetattrs acts differently on 9i and 10g,
   -- and returns incorrect results if the caller doesn&#39;t have read permissions
