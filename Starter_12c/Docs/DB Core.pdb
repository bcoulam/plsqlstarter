<o:DatabasePackageProcedure Id="o475">
<a:ObjectID>4729683B-0B43-4BF2-9A2E-2494DA8D8B91</a:ObjectID>
<a:Name>RENAME_SEQ</a:Name>
<a:Code>RENAME_SEQ</a:Code>
<a:CreationDate>1273769553</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:Definition>l_proc_nm user_objects.object_name%TYPE := &#39;rename_seq&#39;;
   l_obj_nm  user_objects.object_name%TYPE;

BEGIN
   l_obj_nm := UPPER(i_seq_nm);

   assert(LENGTH(i_new_seq_nm) &lt;= 30,
          l_proc_nm || g_sep_char || i_new_seq_nm || &#39; is &#39; ||
          TO_CHAR(LENGTH(i_new_seq_nm) - 30) || &#39; characters too long.&#39;);

   -- See if old sequence exists before trying to rename it
   IF (obj_exists(l_obj_nm, gc_sequence)) THEN
      p(&#39;Renaming sequence &#39; || l_obj_nm || &#39; to &#39; || UPPER(i_new_seq_nm));
      EXECUTE IMMEDIATE &#39;RENAME &#39; || i_seq_nm || &#39; TO &#39; || i_new_seq_nm;

   -- If that fails, see if the new sequence name already exists
   ELSIF (obj_exists(UPPER(i_new_seq_nm), gc_sequence)) THEN
      inf(l_proc_nm || g_sep_char || &#39;Sequence &#39; || UPPER(i_new_seq_nm) || &#39; already exists. This is OK.&#39;);

   -- With no success above, something must be wrong
   ELSE
      err(l_proc_nm || g_sep_char || &#39;Both &#39; || l_obj_nm || &#39; and &#39; || UPPER(i_new_seq_nm) ||
          &#39; do not exist. Check for spelling and assumption errors.&#39;);
   END IF;

END rename_seq</a:Definition>
