<o:DatabasePackageProcedure Id="o1292">
<a:ObjectID>EAF59354-C5BB-43D0-9BCC-BDE3F4CFF809</a:ObjectID>
<a:Name>STR_TO_BOOL</a:Name>
<a:Code>STR_TO_BOOL</a:Code>
<a:CreationDate>1273769568</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>BOOLEAN</a:ReturnType>
<a:Definition>l_proc_nm  app_log.routine_nm%TYPE := gc_pkg_nm||&#39;.str_to_bool&#39;;
BEGIN
   IF (lower(i_str) IN (&#39;true&#39;,&#39;y&#39;,&#39;yes&#39;,&#39;t&#39;,&#39;1&#39;)) THEN
      RETURN TRUE;
   ELSIF (lower(i_str) IN (&#39;false&#39;,&#39;n&#39;,&#39;no&#39;,&#39;f&#39;,&#39;0&#39;)) THEN
      RETURN FALSE;
   ELSIF (i_str IS NULL) THEN
      RETURN NULL;
   ELSE
      logs.err(&#39;str_to_bool does not support [&#39;||i_str||&#39;]&#39;, TRUE, l_proc_nm, $$PLSQL_LINE);
   END IF;
END str_to_bool</a:Definition>
