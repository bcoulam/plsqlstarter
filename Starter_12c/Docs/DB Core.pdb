<o:DatabasePackageProcedure Id="o1134">
<a:ObjectID>A51E3B8C-D61E-4B3B-803C-B407EA50F270</a:ObjectID>
<a:Name>FILL_MSG</a:Name>
<a:Code>FILL_MSG</a:Code>
<a:CreationDate>1273769566</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>VARCHAR2</a:ReturnType>
<a:Definition>l_msg app_msg.msg%TYPE;
BEGIN

   BEGIN
      SELECT msg
        INTO l_msg
        FROM app_msg
       WHERE msg_cd = i_msg_cd;
   EXCEPTION
      WHEN NO_DATA_FOUND THEN
         l_msg := &#39;Unable to retrieve message text for message code: &#39; || i_msg_cd;
   END;

   IF (l_msg IS NOT NULL AND ias_fill.COUNT &gt; 0) THEN
      FOR i IN ias_fill.FIRST .. ias_fill.LAST LOOP
         -- Given the index &quot;i&quot; find the related placeholder in the message and
         -- replace the placeholder with the array&#39;s value at index &quot;i&quot;.
         l_msg := REPLACE(l_msg,
                          i_wrap_char || TO_CHAR(i) || i_wrap_char,
                          ias_fill(i));
      END LOOP;
   END IF;

   RETURN l_msg;

END fill_msg</a:Definition>
<a:BaseProcedure.Function>1</a:BaseProcedure.Function>
<c:Parameters>
