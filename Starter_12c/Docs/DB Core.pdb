<o:DatabasePackageProcedure Id="o866">
<a:ObjectID>62F518EA-AB86-4182-B7F1-BAFA2C2338D9</a:ObjectID>
<a:Name>CONVERT_FILE_TO_BLOB</a:Name>
<a:Code>CONVERT_FILE_TO_BLOB</a:Code>
<a:CreationDate>1273769562</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>BLOB</a:ReturnType>
<a:Definition>l_bfile BFILE;
   l_data  BLOB;
BEGIN
   dbms_lob.createtemporary(lob_loc =&gt; l_data,
                            CACHE   =&gt; TRUE,
                            dur     =&gt; dbms_lob.CALL);
   l_bfile := BFILENAME(i_file_dir, i_file_nm);
   dbms_lob.fileopen(l_bfile, dbms_lob.file_readonly);
   dbms_lob.loadfromfile(l_data, l_bfile, dbms_lob.getlength(l_bfile));
   dbms_lob.fileclose(l_bfile);
   RETURN l_data;
END convert_file_to_blob</a:Definition>
