<o:DatabasePackageProcedure Id="o698">
<a:ObjectID>28C88E67-5DE1-49AA-9A9E-FF90D2F43A74</a:ObjectID>
<a:Name>MINUTES_TO_DHM</a:Name>
<a:Code>MINUTES_TO_DHM</a:Code>
<a:CreationDate>1273769560</a:CreationDate>
<a:Creator>COULAMWA</a:Creator>
<a:ModificationDate>1273769581</a:ModificationDate>
<a:Modifier>COULAMWA</a:Modifier>
<a:ReturnType>VARCHAR2</a:ReturnType>
<a:Definition>l_days      PLS_INTEGER;
   l_hours     PLS_INTEGER;
   l_carryover NUMBER := 0;
BEGIN
   IF (i_mnts IS NULL) THEN
      RETURN NULL;
   END IF;
   l_days      := TRUNC(i_mnts / minutes_per_day); --gives any days involved
   l_carryover := MOD(i_mnts, minutes_per_day); --strips days from remaining hours
   l_hours     := TRUNC(l_carryover / 60); --gives any hours involved
   l_carryover := ROUND(MOD(l_carryover, 60)); --strips hours from remaining minutes

   RETURN TO_CHAR(l_days) || &#39;:&#39; || TO_CHAR(l_hours) || &#39;:&#39; || TO_CHAR(l_carryover,&#39;FM09&#39;);

END minutes_to_dhm</a:Definition>
